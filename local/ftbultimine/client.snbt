{
	# Maximum number of blocks the white outline should be rendered for
	# Keep in mind this may get *very* laggy for large amounts of blocks!
	# Default: 256
	# Range: 0 ~ +∞
	render_outline: 256
	
	# Manual x offset of FTB Ultimine overlay, required for some modpacks
	# Default: -1
	# Range: -∞ ~ +∞
	x_offset: -1
}
